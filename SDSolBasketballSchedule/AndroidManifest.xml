    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.prouty.leagueusa.sdsolschedule.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.prouty.leagueusa.sdsolschedule.UploadFileActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.prouty.leagueusa.sdsolschedule.DivisionListActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.prouty.leagueusa.sdsolschedule.TeamListActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.prouty.leagueusa.sdsolschedule.GameListActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.prouty.leagueusa.sdsolschedule.PhotoListActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.prouty.leagueusa.sdsolschedule.ImagePagerActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.prouty.leagueusa.sdsolschedule.ImageActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>